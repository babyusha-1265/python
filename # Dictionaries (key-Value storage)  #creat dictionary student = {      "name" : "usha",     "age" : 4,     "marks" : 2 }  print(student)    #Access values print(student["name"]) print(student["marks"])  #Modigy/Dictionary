# Dictionaries (key-Value storage)

#creat dictionary
student = { 
    "name" : "usha",
    "age" : 4,
    "marks" : 2
}

print(student)  

#Access values
print(student["name"])
print(student["marks"])

#Modigy/Add
student["age"] = '10'
student["branch"] = "ECE"
print(student)

#loop through dictionary

for key,value in student.items():
    print(key, ":", value)
 
 #Fuctions   
print(student.keys())
print(student.values())
print(len(student))
